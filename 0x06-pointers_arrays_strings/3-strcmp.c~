#include "main.h"
/**
 * _strcmp - a function that compares two strings
 * @dest: input
 * @src: input
 * Return: value
 */
char *_strcmp(char *dest, char *src)
{
int i, sum_1, sum_2;

for (i = 0; (dest[i] != '\0' && src[i] != '\0') && (dest == src); i++)
{
sum_1++;
sum_2++;
}
if (*dest == *src)
{
return 0;
}
else
{
return (*dest - *src);
}
}
